{
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install && npm run postinstall",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "PRISMA_CLIENT_ENGINE_TYPE": "library",
    "NODE_ENV": "production"
  },
  "postbuild": "npm run vercel-postbuild",
  "prisma": {
    "version": "6.6.0"
  },
  "ignoreCommand": "git diff --quiet $VERCEL_GIT_COMMIT_REF $VERCEL_GIT_COMMIT_SHA"
}
