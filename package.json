{
  "name": "coupleplan",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=src/tests --testPathIgnorePatterns=src/tests/integration --reporters=default --reporters=jest-junit --passWithNoTests",
    "test:unit:html": "jest --testPathPattern=src/tests --testPathIgnorePatterns=src/tests/integration --reporters=default --reporters=jest-junit --reporters=jest-html-reporter --passWithNoTests",
    "test:integration": "jest --testPathPattern=src/tests/integration --coverage --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "test:e2e:report": "playwright show-report",
    "test:cdc": "tsx tests/cdc/runner.ts",
    "test:cdc:msw": "jest --testPathPattern=tests/cdc/msw --passWithNoTests",
    "test:cdc:pact": "jest --testPathPattern=tests/cdc/pact --passWithNoTests",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:cdc",
    "vercel-build": "next build"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "isomorphic-dompurify": "^2.31.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.7",
    "next-auth": "^4.24.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.5.1",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250517.0",
    "@eslint/eslintrc": "^3",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-fixed-jsdom": "^0.0.9",
    "jest-html-reporter": "^4.1.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.10.2",
    "node-fetch": "^2.7.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5",
    "vitest": "^3.1.1",
    "wrangler": "^4.15.2"
  },
  "jest-junit": {
    "outputDirectory": "./",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "jest-html-reporter": {
    "pageTitle": "Couple Plan Test Report",
    "outputPath": "test-report.html",
    "includeFailureMsg": true,
    "includeConsoleLog": true,
    "theme": "defaultTheme"
  }
}
